{
  "name": "ragify-js",
  "version": "0.1.1",
  "description": "A powerful and flexible Retrieval-Augmented Generation (RAG) library for Node.js and TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint \"src/**/*.ts\" \"examples/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" \"examples/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepare": "yarn build && husky install",
    "dev": "ts-node-dev --respawn src/index.ts",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "rag",
    "retrieval-augmented-generation",
    "ai",
    "nlp",
    "embeddings",
    "vector-store",
    "typescript",
    "openai",
    "qdrant"
  ],
  "author": "Manjot Singh",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manjotdhiman/ragify-js"
  },
  "bugs": {
    "url": "https://github.com/manjotdhiman/ragify-js/issues"
  },
  "homepage": "https://github.com/manjotdhiman/ragify-js#readme",
  "dependencies": {
    "@qdrant/js-client-rest": "^1.7.0",
    "@types/pdf-parse": "^1.1.4",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "openai": "^4.28.0",
    "pdf-parse": "^1.1.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.5",
    "globals": "^16.0.0",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
